{
  "name": "@tszip/tszip",
  "type": "module",
  "version": "0.2.6",
  "author": "C. Lewis <ctj.lewis@icloud.com>",
  "description": "Backwards-compatible compiler for TypeScript libraries.",
  "license": "MIT",
  "homepage": "https://github.com/tszip/tszip",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tszip/tszip"
  },
  "bin": {
    "tszip": "./dist/index.js"
  },
  "files": [
    "dist",
    "templates"
  ],
  "engines": {
    "node": ">=14"
  },
  "keywords": [
    "react",
    "typescript",
    "bundle",
    "rollup"
  ],
  "bugs": {
    "url": "https://github.com/tszip/tszip/issues"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "prebootstrap": "yarn clean",
    "bootstrap": "tsc && rollup -c --silent && yarn build && yarn build",
    "prepare": "yarn bootstrap",
    "dev": "yarn bootstrap && node dist/index.js watch",
    "build": "node dist/index.js build",
    "postbuild": "chmod +x dist/index.*js",
    "lint": "yarn build && yarn lint:post-build",
    "lint:post-build": "node dist/index.js lint ./ --ignore-pattern 'test/e2e/fixtures/lint'",
    "test": "yarn build && node dist/index.js test",
    "release": "np",
    "deduplicate": "yarn-deduplicate -s fewer yarn.lock",
    "deduplicate:check": "yarn-deduplicate -s fewer yarn.lock --list --fail"
  },
  "dependencies": {
    "@babel/core": "^7.4.4",
    "@babel/helper-module-imports": "^7.0.0",
    "@babel/parser": "^7.11.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.15.4",
    "@babel/traverse": "^7.11.5",
    "@optimize-lodash/rollup-plugin": "^1.0.0",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-typescript": "^8.2.5",
    "@tszip/esm-require": "^0.0.1",
    "@tszip/resolve-imports": "latest",
    "@tszip/rollup-config": "^0.0.2",
    "@types/jest": "^26.0.24",
    "@types/rollup-plugin-postcss": "^3.1.4",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "ansi-escapes": "^4.2.1",
    "asyncro": "^3.0.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-polyfill-regenerator": "^0.2.2",
    "babel-plugin-transform-rename-import": "^2.3.0",
    "camelcase": "^6.0.0",
    "chalk": "^4.0.0",
    "enquirer": "^2.3.4",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.8.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^4.2.0",
    "execa": "^5.1.1",
    "fs-extra": "^10.0.0",
    "glob-promise": "^4.2.0",
    "jest": "^27.1.0",
    "jest-watch-typeahead": "^0.6.4",
    "jpjs": "^1.2.1",
    "lodash-es": "^4.17.21",
    "lodash.merge": "^4.6.2",
    "ora": "^5.4.1",
    "pascal-case": "^3.1.1",
    "postcss": "^8.3.6",
    "prettier": "^2.3.2",
    "progress-estimator": "^0.3.0",
    "regenerator-runtime": "^0.13.7",
    "resolve": "^1.20.0",
    "resolve.exports": "^1.0.2",
    "rollup": "^2.52.8",
    "rollup-plugin-preserve-shebang": "^1.0.1",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "sade": "^1.4.2",
    "semver": "^7.1.1",
    "shelljs": "^0.8.4",
    "tiny-glob": "^0.2.6",
    "ts-jest": "^27.0.5",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@types/autoprefixer": "^10.2.0",
    "@types/cssnano": "^4.0.1",
    "@types/eslint": "^7.2.14",
    "@types/fs-extra": "^9.0.1",
    "@types/lodash": "^4.14.161",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^16.0.1",
    "@types/react": "^17.0.14",
    "@types/rollup-plugin-json": "^3.0.2",
    "@types/sade": "^1.6.0",
    "@types/semver": "^7.1.0",
    "@types/shelljs": "^0.8.5",
    "@types/styled-components": "^5.0.1",
    "autoprefixer": "^10.3.4",
    "babel-plugin-replace-identifiers": "^0.1.1",
    "cssnano": "^5.0.8",
    "doctoc": "^2.0.1",
    "husky": "^7.0.1",
    "np": "^7.5.0",
    "pretty-quick": "^3.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "rollup-plugin-postcss": "^4.0.1",
    "styled-components": "^5.0.1",
    "tiny-invariant": "^1.1.0",
    "tiny-warning": "^1.0.3",
    "yarn-deduplicate": "^3.1.0"
  },
  "jest": {
    "extensionsToTreatAsEsm": [
      ".ts"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern '!test/tests/lint/**' && yarn lint && yarn deduplicate:check && doctoc README.md"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
